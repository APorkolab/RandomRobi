{
  "name": "randomrobi-backend",
  "version": "1.0.0",
  "description": "Backend for Random Robi project",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "NODE_ENV=test mocha --exit --recursive test/",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "nyc npm run test",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "docker:build": "docker build -t randomrobi-backend .",
    "docker:run": "docker run -p 3000:3000 randomrobi-backend",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all"
  },
  "dependencies": {
    "axios": "^1.8.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "mysql2": "^3.11.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "puppeteer": "^23.5.0",
    "randomrobi-backend": "file:",
    "sequelize": "^6.37.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.14.2"
  }
  "devDependencies": {
    "@types/node": "^20.8.0",
    "chai": "^4.3.8",
    "chai-http": "^4.4.0",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "mocha": "^10.7.3",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.6.1",
    "sinon": "^16.1.0",
    "supertest": "^6.3.3"
  },
  "author": "Adam Dr. Porkolab",
  "license": "ISC"
}
